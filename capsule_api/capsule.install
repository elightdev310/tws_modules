/**
 * @file
 *
 * Provide functions to install and uninstall the Capsule API module.
 */

/**
 * Implementation of hook_uninstall().
 */
function capsule_uninstall() {
  variable_del('capsule_url_key');
  variable_del('capsule_user_token');
}
